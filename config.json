{
  "email": {
    "server": "smtp.gmail.com",
    "port": 587,
    "user": "bluez.test.bot@gmail.com",
    "starttls": true,
    "default-to": "linux-bluetooth@vger.kernel.org",
    "only-maintainers": true,
    "maintainers": [
      "tedd.an@intel.com"
    ]
  },
  "patchwork": {
    "url": "https://patchwork.kernel.org",
    "project_name": "Bluetooth"
  },
  "space_details": {
    "kernel": {
      "include": [
        "Bluetooth:"
      ],
      "exclude": [
        "BlueZ",
        "pull request"
      ],
      "upstream": "https://git.kernel.org/pub/scm/linux/kernel/git/bluetooth/bluetooth-next.git/",
      "ci": {
        "CheckPatch": {
          "ignore": "UNKNOWN_COMMIT_ID"
        },
        "TestRunner": {
          "tester-list": [
            "l2cap-tester",
            "iso-tester",
            "bnep-tester",
            "mgmt-tester",
            "rfcomm-tester",
            "sco-tester",
            "ioctl-tester",
            "mesh-tester",
            "smp-tester",
            "userchan-tester"
          ]
        }
      }
    },
    "user": {
      "include": [
        "BlueZ"
      ],
      "exclude": [
        "Bluetooth:",
        "pull request"
      ],
      "upstream": "https://git.kernel.org/pub/scm/bluetooth/bluez.git/"
    }
  }
}
